{
  "name": "@minglabs-sz/cli",
  "version": "0.0.1",
  "description": "MingLabs CLI Tool",
  "author": "Jade",
  "bin": {
    "mingcli": "./bin/run"
  },
  "homepage": "https://github.com/zjhiphop/@ming-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "zjhiphop/@ming-cli",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "directories": {
    "src": "src",
    "bin": "bin",
    "test": "test"
  },
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "cli-ux": "^6.0.8",
    "fast-extract": "^0.3.1",
    "inquirer": "^8.2.0",
    "kolorist": "^1.5.1",
    "nock": "^13.2.2",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/inquirer": "^8.1.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "mingcli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      },
      "vue": {
        "description": "Create a new vue web project"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "TEST=* mocha --forbid-only \"test/**/*.test.ts\"",
    "test-cli": "TEST=* mocha --forbid-only \"__tests__/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/zjhiphop/@ming-cli/issues",
  "keywords": [
    "oclif"
  ],
  "dirname": "mingcli",
  "types": "dist/index.d.ts"
}
